# 本地開發環境設定
APP_NAME=MyApp
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost

# 資料庫設定
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=myapp_local
DB_USERNAME=root
DB_PASSWORD=

# 快取設定
CACHE_DRIVER=file
REDIS_URL=redis://localhost:6379/0

# 會話設定
SESSION_DRIVER=file
SESSION_LIFETIME=120

# 日誌設定
LOG_LEVEL=debug

# 郵件設定
MAIL_MAILER=log

# 安全性設定（請在生產環境更換）
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here
